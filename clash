    filter: "^(?=.*((?i)直连|(\b(AD)\b)))(?!.*((?i)回国|校园|游戏|🎮|(\b(GAME)\b))).*$"
    proxies:
      - DIRECT
      - REJECT
  - name: "拒绝·手动选择"
    type: select
    use:
      - "西数·订阅"
    filter: "^(?=.*((?i)广告|(\b(AD)\b)))(?!.*((?i)回国|校园|游戏|🎮|(\b(GAME)\b))).*$"
    proxies:
      - DIRECT
      - REJECT

rule-providers:
